<Window x:Class="FrontDeskApp.Views.CheckOutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FrontDeskApp.Views"
        mc:Ignorable="d"
        Title="Check Out" Height="75" Width="290">
    <Grid>
        <TextBox x:Name="ResNrBox" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="Reservation Nr" VerticalAlignment="Top" Width="90"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Height="23" Margin="110,10,0,0" VerticalAlignment="Top" Width="75" Click="CancelButton_Click"/>
        <Button Content="Check Out" HorizontalAlignment="Left" Height="23" Margin="195,10,0,0" VerticalAlignment="Top" Width="75" Click="CheckOutButton_Click"/>

    </Grid>
</Window>
